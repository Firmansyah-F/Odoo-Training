<odoo>
    <data noupdate='1'>
        <record id="ir_seq_service_orders" model="ir.sequence">
			<field name="name">Service Order Sequence</field>
			<field name="code">service.service.sequence</field>
			<field name="prefix">SERVICE/%(y)s/</field>
			<field name="suffix">/XTI</field>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
			<field name="implementation">standard</field>
		</record>
	</data>
</odoo>